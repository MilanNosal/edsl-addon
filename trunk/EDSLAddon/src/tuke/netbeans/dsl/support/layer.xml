<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-java">
                <folder name="FoldManager">
                    <file name="tuke-netbeans-dsl-support-EDSLFoldManagerFactory.instance">
                        <attr name="position" intvalue="100"/>
                    </file>
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance">
                        <attr name="position" intvalue="300"/>
                    </file>
                    <file name="org-netbeans-modules-java-editor-fold-JavaElementFoldManagerFactory.instance">
                        <attr name="position" intvalue="200"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    
    <folder name="Templates">
        <folder name="DSL">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="tuke.netbeans.entity_dsl.Bundle"/>
            
            <file name="Entities.java" url="nbresloc:/tuke/netbeans/entity_dsl/Entities.java.template">
                <attr name="template" boolvalue="true"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="tuke.netbeans.entity_dsl.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/tuke/netbeans/entity_dsl/entity.png"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/tuke/netbeans/entity_dsl/Description.html"/>
                <!--Use this line only if your template makes use of the FreeMarker template language:-->
                <attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
            </file>
            
        </folder>
    </folder>
</filesystem>
